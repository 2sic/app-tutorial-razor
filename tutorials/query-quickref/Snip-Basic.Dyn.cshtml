@inherits Custom.Hybrid.Razor14
@{/*! Tut.Tabs=ViewConfig */
  var viewCnf = DynamicModel.Tutorial.ViewConfig;
  var Data = viewCnf.Query(query: "QuickRef-Persons-Selected");
/*!*/}
<h4>Loop persons in Query of this view</h4>
<ul>
  @foreach (var person in AsList(Data)) {
    <li>
      @Kit.Image.Picture(person.Mugshot, settings: "Square", width: 50, imgClass: "rounded-circle")
      @person.FirstName @person.LastName
    </li>
  }
</ul>
