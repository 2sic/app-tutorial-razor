@inherits Custom.Hybrid.Razor14
@using ToSic.Razor.Blade;
@using System.Linq;
@using ToSic.Eav.DataSources;
@{/*! Tut.Tabs=file:../WithConfig.cs, ViewConfig */
/*!*/}
@{
  var viewCnf = DynamicModel.Tutorial.ViewConfig;
  var Data = viewCnf.Query(query: "DynamicDataSourceWithConfig") as ToSic.Eav.DataSource.IDataSource;
}
<h3>Data in the Query (@queryWithConfig.List.Count())</h3>
<ul>
  @foreach (var item in AsList(queryWithConfig)) {
    <li>
      <strong>@item.Title</strong> - Favorite Color: @item.FavoriteColor
    </li>
  }
</ul>