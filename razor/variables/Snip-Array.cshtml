@inherits Custom.Hybrid.RazorTyped

@{
  var dogs = new string[] { "hound", "dackel", "doggy" };
}
<div>
  Dog Count: @dogs.Length <br>
  Dog[0]: @dogs[0] <br>
  Dog First (with LINQ): @dogs.First() <br>
  Dog Last (with LINQ): @dogs.Last() <br>
</div>

<ol>
  @for (int i = 0; i < dogs.Length; i++) {
    <li>Dog @i: @dogs[i]</li>
  }
</ol>
