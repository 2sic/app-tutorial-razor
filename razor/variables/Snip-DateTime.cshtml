@inherits Custom.Hybrid.RazorTyped
@{ var Sys = GetCode("/shared/Helpers.cs").Init(this); }

@* <h3>Numbers and Dates</h3> *@

@{ var snip = Sys.SourceCode.OutputBoxAndSnippet(); }
@snip.SnipStart()
  @{
    var age = 27;
    var birthday = new DateTime(1999, 09, 09);
  }
  Age: @age <br>
  Birthday: @birthday <br>
  Birthday nicer using ToString("d"): @birthday.ToString("d") <br>
  Birthday as ISO ToString("yyyy-MM-dd"): @birthday.ToString("yyyy-MM-dd")
@snip.SnipEnd()

@* @{ var snip = Sys.SourceCode.QuickRef(item: MyModel.Item("Item")); }
@snip.SnipStart()
  <h3>@MyHeader.Title</h3>
  <ul>
    @foreach (var item in MyItems) {
      <li>
        @item.Title
      </li>
    }
  </ul>
@snip.SnipEnd() *@


