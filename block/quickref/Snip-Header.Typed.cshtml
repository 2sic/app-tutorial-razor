@inherits Custom.Hybrid.RazorTyped
@{/*! Tut.Tabs=ViewConfig */
  // Patch so the tutorial works reliably even if the main module had data added which are not a person
  // So we're simulating that `MyData` is the assigned query for this view
  var tutCode = MyModel.Code("Tutorial");
  var MyItems = AsItems(tutCode.SimulateViewList(query: "QuickRef-Content-Simulation") as object);
  var MyHeader = AsItem(tutCode.SimulateViewHeader(query: "QuickRef-Content-Simulation", stream: "Header") as object);
/*!*/}
<h3>@MyHeader.Title</h3>
<ul>
  @foreach (var item in MyItems) {
    <li>
      @item.Title
    </li>
  }
</ul>


