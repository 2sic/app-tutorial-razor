@inherits Custom.Hybrid.RazorTyped
@{
  // Sys helps generate code snippets etc.
  var Sys = GetCode("/shared/Helpers.cs").Init(this);
  // The accordion is an external helper file, so we must treat it as dynamic
  var acc = MyModel.Get("Accordion") as dynamic;
  // variable to hold each section config
  dynamic accSection;
  dynamic qref;
}

  @((accSection = acc.Section("quickref-page-set")).Start())
    @Html.Partial("QuickRef-Page-Set.cshtml")
  @accSection.End()

  @((accSection = acc.Section("quickref-page-opengraph")).Start())
    @Html.Partial("QuickRef-Page-OpenGraph.cshtml")
  @accSection.End()

  @((accSection = acc.Section("quickref-context")).Start())
    @Html.Partial("QuickRef-Context Typed.cshtml")
  @accSection.End()