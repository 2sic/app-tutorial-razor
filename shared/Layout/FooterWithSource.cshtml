@inherits Custom.Hybrid.Razor14
@{
  // Expect the calling page to give us it's Sys object
  // which contains a lot of shared functionality
  var Sys = DynamicModel.Sys;
}

@* Get the toolbar for: < previous | # Current | Next > *@
@Html.Partial("./NavigationTutorials.cshtml", new { Sys = Sys })

@* Show the source code of the current file. The Sys object already knows what file it is *@
@Sys.SourceCode.ShowFile("")